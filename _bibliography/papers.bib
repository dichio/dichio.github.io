@article{zeng2020,
  title={Global analysis of more than 50,000 SARS-CoV-2 genomes reveals epistasis between eight viral genes},
  author={Zeng, Hong-Li and Dichio, Vito and Rodr{\'\i}guez Horta, Edwin and Thorell, Kaisa and Aurell, Erik},
  journal={Proceedings of the National Academy of Sciences},
  abbr = {PNAS},
  volume={117},
  number={49},
  pages={31519--31526},
  year={2020},
  publisher={National Acad Sciences},
  html = {https://www.pnas.org/doi/abs/10.1073/pnas.2012331117},
  image = {/assets/img/img_bibliography/zeng2020.png},
  bibtex_show = {true},
  abstract = {We apply Direct Coupling Analysis (DCA) to a large dataset of SARS-CoV-2 genomes to infer the strongest epistatic coupling between different genes. Our analysis unveils significant interactions between the loci in the viral genes ORF3a and nsp2, nsp12, and nsp6, as well as between ORF8 and nsp4, and among the loci in the genes nsp2, nsp13, and nsp14.},
}

@article{zeng2021,
  title={Inferring epistasis from genomic data with comparable mutation and outcrossing rate},
  author={Zeng, Hong-Li and Mauri, Eugenio and Dichio, Vito and Cocco, Simona and Monasson, Remi and Aurell, Erik},
  journal={Journal of Statistical Mechanics: Theory and Experiment},
  abbr = {JSTAT},
  volume={},
  number={8},
  pages={083501},
  year={2021},
  publisher={IOP Publishing},
  html = {https://iopscience.iop.org/article/10.1088/1742-5468/ac0f64},
  image = {/assets/img/img_bibliography/zeng2021.png},
  bibtex_show = {true},
  abstract = {We address the problem of inferring fitness in an evolutionary dynamics from multiple snapshots of the genotype distribution in a population that is in a quasi-linkage equilibrium regime. We extend the current state-of-the-art methods and leverage a recently proposed technique that uses a Gaussian approximation for the probability distribution of the genome. The results are validated through in silico experiments.},
}

