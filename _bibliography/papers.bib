@article{dichio2022,
  title={Statistical models of complex brain networks: a maximum entropy approach},
  author={Dichio, Vito and Fallani, Fabrizio De Vico},
  journal={arXiv preprint arXiv:2209.05829},
  abbr={arXiv},
  year={2022},
  html = {https://arxiv.org/abs/2209.05829},
  image = {/assets/img/img_bibliography/dichio2022.png},
  bibtex_show = {true},
  abstract = {We focus on Exponential Random Graph Models (ERGMs) and thir applications to network neuroscience. ERGMs are a family of maxent inference models designed for network data. These models can be utilized to characterize the state of a network, distinguish between different states, and forecast future outcomes. We present a brief overview of the method, examine current applications in brain networks, and provide a critical discussion.}
}

